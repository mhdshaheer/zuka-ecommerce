<link rel="stylesheet" href="/css/admin-login.css">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-ZOa+mrFOMRRewOrTAYtnlOSzPpPG5xrhK4N+NE4lVoUMyH45WcsdS/xBfO5qKT9g" crossorigin="anonymous">


<form action="/admin/login" method="post">
<div class="loginDiv" id="loginDiv">
	<h1>Admin Login</h1>
	<div class="inputField">
		<input type="text" id="username" required aria-invalid="false" name="email" />
		<label>Email</label>
		
	</div>
	<div class="inputField">
		<input type="password" id="password" required aria-invalid="false" name="password"/>
		<label>Password</label>
	</div>
	
	<!-- <a href="/admin/dashboard"> -->
		<button type="submit" id="login" class="login" onclick="loginFun('<%=message%>')">Login</button>
	<!-- </a> -->
</div>
</div>
</form>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-kxZQV8j6AKJ1iBTPZ8a32X63ks74yNq0UUiYj3EdqxYklRHaU99d2sCQFnYlUG7w" crossorigin="anonymous"></script>
